from math import ceil
#принимает в качестве аргумента целое число и выводит True если оно простое
def is_prime(num):
    if num == 1:
        return False
    elif num == 2:
        return True
    else:
        for k in range(2, ceil(num**0.5) + 1):
            if num % k == 0:
                return False
        return True
