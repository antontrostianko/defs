def get_gcd(a, b): #НОД
    while a != b:
        if a > b:
            a -= bhttps://github.com/antontrostianko/defs/blob/main/get_lcm
        else:
            b -= a
    return a
   
def get_gcd1(a, b):
    a, b = max(a, b), min(a, b)   
    r = a % b
    k = 0
    gcd = a
    while r != 0:
        k = b % r
        gcd = r
        r = k
    return gcd
